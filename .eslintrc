{
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "linebreak-style": 0,
    "indent": 0,
    "no-undef": 0,
    "no-multi-spaces": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "object-shorthand": 0,
    "no-underscore-dangle": 0,
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-plusplus": 0,
    "brace-style": 0,
    "import/prefer-default-export": 0,
    "no-use-before-define": 0,
    "space-infix-ops": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0
  }
}